      <memberdef kind="function" id="dd/d26/classsamchon_1_1namtree_1_1NTEntityGroup_1a550f33fdf4fb2c72d96f325b4b5dafe8" prot="public" static="no" const="yes" explicit="no" inline="no" virt="pure-virtual">
